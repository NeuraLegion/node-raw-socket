{
  "name": "@neuralegion/raw-socket",
  "version": "2.0.0",
  "description": "Raw sockets for Node.js.",
  "main": "index.cjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.cjs"
      }
    }
  },
  "files": [
    "index.cjs",
    "index.mjs",
    "index.d.ts",
    "prebuilds",
    "src",
    "binding.gyp"
  ],
  "directories": {
    "example": "example"
  },
  "gypfile": true,
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "nan": "^2.24.0",
    "node-gyp-build": "^4.8.3"
  },
  "contributors": [
    {
      "name": "Stephen Vickers",
      "email": "stephen.vickers@nospaceships.com"
    },
    {
      "name": "NoSpaceships Ltd",
      "email": "hello@nospaceships.com"
    },
    {
      "name": "Artem Derevnjuk",
      "email": "artem.derevnjuk@neuralegion.com"
    }
  ],
  "scripts": {
    "semantic-release": "semantic-release",
    "build": "tsdown",
    "build:gyp": "prebuildify --napi false --strip --target 20.20.0 --target 21.7.3 --target 22.22.0 --target 23.11.1 --target 24.13.0 --target 25.3.0",
    "build:gyp-cross": "prebuildify-cross --napi false --strip --target 20.20.0 --target 21.7.3 --target 22.22.0 --target 23.11.1 --target 24.13.0 --target 25.3.0",
    "prepublishOnly": "npm run build",
    "install": "node-gyp-build",
    "test": "node --test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeuraLegion/node-raw-socket.git"
  },
  "bugs": {
    "url": "https://github.com/NeuraLegion/node-raw-socket/issues"
  },
  "keywords": [
    "checksum",
    "checksums",
    "htonl",
    "htons",
    "net",
    "network",
    "ntohl",
    "ntohs",
    "raw",
    "raw-socket",
    "raw-sockets",
    "socket",
    "sockets"
  ],
  "author": "NoSpaceships Ltd <hello@nospaceships.com>",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "prebuildify": "^6.0.1",
    "prebuildify-cross": "^5.1.1",
    "semantic-release": "^25.0.2",
    "tsdown": "^0.20.0-beta.3"
  },
  "overrides": {
    "prebuildify": {
      "node-abi": "^4.25.0"
    }
  }
}
